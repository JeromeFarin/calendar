<header>
    <div class="jumbotron">
        <h1 class="display-3">Calendar API</h1>
        <div id="user_manage">
            {% if app.user.email is defined %}
                <a href="{{ path('security_logout') }}">Logout</a>
            {% else %}
                <a href="{{ path('security_login') }}">Login</a> | <a href="{{ path('security_register') }}">Register</a>
            {% endif %}
            
        </div>
    </div>
</header>